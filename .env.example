# AI API 配置
AI_API_BASE_URL=https://your-ai-api.com
AI_API_KEY=your_api_key_here

# OpenAI 配置（如果使用 OpenAI）
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_API_BASE=https://api.openai.com/v1

# 或其他 AI 服务配置
# Claude API
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# 应用配置
PORT=8000
NODE_ENV=development

# Mock 配置
MOCK=true

# 百度翻译
BAIDU_TRANSLATE_APPID=your_appid
BAIDU_TRANSLATE_SECRET=your_secret

# 腾讯会议
TENCENT_MEETING_APP_ID=your_app_id
TENCENT_MEETING_SDK_ID=your_sdk_id
TENCENT_MEETING_SECRET_ID=your_secret_id
TENCENT_MEETING_SECRET_KEY=your_secret_key

# 阿里云 OSS
OSS_REGION=oss-cn-hangzhou
OSS_ACCESS_KEY_ID=your_access_key_id
OSS_ACCESS_KEY_SECRET=your_access_key_secret
OSS_BUCKET=your_bucket_name
OSS_ENDPOINT=your_endpoint


# LLM 基础配置
LLM_PROVIDER=deepseek
LLM_API_KEY=sk-你的deepseek密钥
LLM_BASE_URL=https://api.deepseek.com/v1
LLM_MODEL=deepseek-chat
